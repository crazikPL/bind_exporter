# Prometheus Bind Exporter defaults
#
START=yes

#
STARTUP_ARGS=' \
      -bind.stats-groups "server,view,tasks" \
      -bind.pid-file {{ prometheus_bind_exporter_pid_path }}/bind_exporter.pid \
      -bind.stats-url {{ prometheus_bind_exporter_targeturl }} \
      -log.level {{ prometheus_bind_exporter_loglevel }} \
      -log.format "logger:syslog?appname=bind_exporter&local=7" \
      -web.listen-address {{ prometheus_bind_exporter_weblisten }} \
      '
